<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--logo-->
    <font android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/typo_ssangmundongb"
        app:font="@font/typo_ssangmundongb"
        app:fontStyle="normal"
        app:fontWeight="400"/>

    <!--normal-->
    <font android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/nanumgothic"
        app:font="@font/nanumgothic"
        app:fontStyle="normal"
        app:fontWeight="400"/>

    <!--bold-->
    <font android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/nanumgothicbold"
        app:font="@font/nanumgothicbold"
        app:fontStyle="normal"
        app:fontWeight="700"/>

    <!--extra bold-->
    <font android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/nanumgothicbold"
        app:font="@font/nanumgothicextrabold"
        app:fontStyle="normal"
        app:fontWeight="1000"/>

</font-family>